    "flagWords": [],
    "ignorePaths": [
        "package.json",
        "yarn.lock",
        "tsconfig.json",
        "node_modules/**",
        "src/@*",
        "src/app/mock-api/**",
        "*.config.js",
        "*.conf.js",
        ".*rc*",
        ".*ignore",
        "*.json",
        "CHANGELOG.md"
    ],
