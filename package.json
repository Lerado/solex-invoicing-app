{
    "name": "solex-invoicing-app",
    "version": "0.1.0",
    "description": "A desktop client in charge of Solex invoicing service",
    "author": {
        "name": "Mohlah Solutions",
        "email": "contact@mohlahsolutions.com",
        "url": "https://mohlahsolutions.com"
    },
