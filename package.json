    "private": true,
    "scripts": {
        "ng": "ng",
        "tauri": "tauri",
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "fix:lint": "ng lint --fix",
        "test:lint": "ng lint --quiet",
        "test:spelling": "cspell \"{{README,CONTRIBUTING}.md,src/**/*.{ts,html}}\" --no-progress",
        "test:ci": "npm run test:lint && npm run test:spelling && ng test --no-watch --no-progress --browsers ChromiumHeadlessCI",
        "test:app": "ng test",
        "test": "npm run test:lint && npm run test:spelling && ng test --no-watch --no-progress",
        "release": "standard-version && git flow release finish -n",
        "reset-hard": "git clean -dfx && git reset --hard && npm run",
        "prepare-release": "npm run reset-hard && npm run test"
    },
